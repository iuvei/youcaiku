.cef{color: #ef403b;}
.c1a{color: #1a8cfe;}
page{background: #f4f4f4;}
.cinfo_top,.cinfo_dom{background: #fff;position: relative;z-index: 10}
.cinfo_top_imt{position: relative;}
.cinfo_top_imt image{width: 100%;}
.cinfo_top_imt .imt_text{position: absolute;font-size: 1.2rem;color: #fff;width: 94%;bottom:0.6rem;left: 3%;white-space: nowrap;text-overflow:ellipsis;overflow: hidden;}
.cinfo_top_text{display: flex;display: -webkit-flex;justify-content:space-between;text-align: center;padding:0.8rem;border-bottom:1px solid #e6e6e6;}
.cinfo_top_text .vt_star{display: flex;display: -webkit-flex;align-items:center;}
.cinfo_top_text .vt_star image{width:0.7rem;height:0.7rem;margin:0 3px;}
.cinfo_top_text .vt_top{height: 1.2rem;line-height: 1.2rem;margin-bottom: 0.3rem;}
.cinfo_top_text .vt_dom{font-size: 0.8rem;color: #666;}
.cinfo_top_link{padding: 0.8rem 1.8rem 0.8rem 0.8rem;position: relative;display: flex;display: -webkit-flex;flex-wrap:wrap;border-bottom:1px solid #e6e6e6;color: #666;}
.cinfo_top_link .link_info{margin-left: 0.8rem;white-space: nowrap;text-overflow: ellipsis;overflow: hidden;width: 13.0rem;color: #212121;}
.cinfo_top_link image{width: 0.6rem;height: 1.0rem;position: absolute;right: 0.6rem;top:50%;margin-top: -0.5rem;}

/*  */
.cinfo_dom{margin-top: 0.5rem;}
.cinfo_content{padding:0 0.8rem 0.6rem;display: none;}
.cinfo_content.active{display: block;}
.tab-h{height:90rpx;width: 100%; box-sizing: border-box;line-height:90rpx;font-size: 1.0rem; white-space: nowrap;border-bottom:1px solid #e6e6e6;display: flex;display: -webkit-flex;color: #666;}
.listText{text-align: center;font-size: 0.85rem;}
.tab-item{margin:0 30rpx;display: inline-block;}
.tab-item.active{color: #212121;position: relative;}
.tab-item.active:after{ content: "";display: block;height:3px;width:100%;background: #1a8cfe;position: absolute; bottom: 0;left:0;border-radius: 5px;}
.item-ans{width:47%;float: left;margin:3% 0 0 2%;overflow: hidden;}
.avatar{width:100%;}
.avatar .img{width:100%;}
/* .avatar .doyen{width: 40rpx;height: 40rpx;position: absolute;bottom: -2px;right: 20rpx;} */
/* .expertInfo{font-size: 12px;flex-grow: 2;color: #B0B0B0;line-height: 1.5em;} */
.expertInfo .name{font-size:0.75rem;margin:0.2rem 0;white-space: nowrap;overflow: hidden;text-overflow: ellipsis;}
.expertInfo .tag{font-weight: bold;font-size: 0.77rem;}
 .askBtn{ width: 120rpx;height: 60rpx;line-height: 60rpx;text-align: center;font-size: 14px; border-radius: 60rpx;border: 1px solid #4675F9; color:#4675F9;}
.tab-content{padding:0.2rem 0.8rem 1.0rem;height:100%;background: #fff;position:relative;z-index: 10}
.tab-content scroll-view{height:100%;}
/* 章节/任务 */
.zj_lt{padding-top:1.5rem;}
.zj_lt.active{color: #1a8cfe;}
.zj_lt .zj_url{position: relative;display: flex;display: -webkit-flex;justify-content: space-between;}
.zj_lt .zj_url .name{margin-left: 0.6rem;}
.zj_lt .zj_url image{width: 1.2rem;height: 1.2rem;}
.zj_lt .zj_lt_text{width:14.0rem;white-space: nowrap;text-overflow: ellipsis;overflow: hidden;}
.zj_rw{color: #666;}
.zj_lt_state.end{color: #1a8cfe;}
.zj_lt_state.ing{color:#ef403b;}
/*底部弹窗*/
.parameter_beij{background: rgba(0,0,0,0.5);width: 100%;height: 100%;position: fixed;left: 0;top: 0;opacity: 0;z-index: -1;
-webkit-transition: all 400ms ease-in-out;
transition: all 400ms ease-in-out;
}
.parameter_beij.opt{opacity: 1.0;z-index: 9;}
.parameter_content{width: 100%;background: #fff;position: fixed;left: 0;bottom:-100%;border-top-left-radius: 0.8rem;z-index: -1;border-top-right-radius: 0.8rem;padding: 0.8rem;
-webkit-box-sizing: border-box; 
 box-sizing: border-box;
 -webkit-transition: all 400ms ease-in-out;
 transition: all 400ms ease-in-out;
 }
.parameter_content.opt{bottom: 0;z-index:10;}
.parameter_title{padding-bottom:0.5rem;border-bottom:1px solid #1a8cfe;display: flex;display: -webkit-flex;justify-content: space-between;color: #1a8cfe;font-weight: bold;}
.parameter_title image{width: 1.3rem;height: 1.3rem;}
.parameter_list .lt{display: flex;display: -webkit-flex;height:3.0rem;line-height:3.0rem;border-bottom:1px solid #e6e6e6;color: #666;}
.parameter_list .lt_name{width:5.0rem;color: #282828;}
.parameter_btn{margin-top: 1.0rem;}
.parameter_btn button{font-size: 1.0rem;color: #fff;background: #1a8cfe;border-radius:2.0rem;}
/* 问答 */
.ans_ct,.pingjia_lt{padding:1.0rem 0;border-bottom: 1px solid #e6e6e6;color: #595959;}
.ans_ct,.ans_ct navigator{justify-content:space-between;}
.ans_ct .ans_info{width:17.5rem;}
.ans_top .icon{width: 1.2rem;height: 1.2rem;}
.ans_top .ans_info{height:2.0rem;background: #f5f5f5;border-radius: 1.5rem;width: 16.5rem;padding:0 0.5rem;margin-top: 0.3rem;}
/* .ans_imt,.pingjia_imt{margin-right:0.5rem;} */
.ans_imt image,.pingjia_imt image{width:2.5rem;height:2.5rem;border-radius: 50%;}
.ans_list .name{color: #212121;width: 10.0rem;}
.ans_list .total{font-size: 0.8rem;margin-top: 0.15rem;color: #a3a3a3;}
.ans_list .total_num{padding:0 0.3rem;}
.ans_list .ans_time{font-size: 0.8rem;margin:0.3rem 0;color: #a3a3a3;}
/* 作业 */
.zuoye_lt{padding:1.0rem 0;border-bottom: 1px solid #e6e6e6;color: #595959;}
.zuoye_info .title{margin-bottom: 0.6rem;width: 14.0rem;}
.zuoye_info .name{color: #9e9e9e;font-size: 0.8rem;}
/* 周报 */
.weekly .ans_top .ans_info{width: 100%;}
.weekly .ans_top .ans_info input{width:88%;}
.weekly .ans_ct{border-bottom: none;}
.weekly_lt{color: #595959;margin-bottom: 1.0rem;}
.weekly_state.on{color: #1A8CFE;}
/* 讨论组 */
.discuss_lt{padding:0.8rem 0;border-bottom:1px solid #e6e6e6;}
.discuss_text{background: #f5f5f5;color: #555;padding:0.2rem;margin-bottom: 0.6rem;}
.discuss_dom{color: #979797;font-size: 0.8rem;}
/* 评价 */
.pingjia_none{text-align: center;color: #5a5a5a;font-size: 0.85rem;}
.pingjia_none .text{margin:4.5rem 0 1.2rem;}
.pingjia_none navigator{display: block;height:1.5rem;line-height:1.5rem;width:6.5rem;border:1px solid #1a8cfe;border-radius: 1.5rem;margin: 0 auto;color:#1a8cfe;}
.pingjia_info{width: 17.5rem;}
.pingjia_info .title_name{color: #212121;}
.pingjia_info .title_star image{display: block;width:0.75rem;height:0.75rem;margin:0 0.2rem;}
.pingjia_info .time{color: #979797;font-size: 0.85rem;}
.pingjia_info .info_lt{margin: 0.4rem 0;flex-wrap:wrap;}
.pingjia_info .info_lt .lt{width:31%;height: 1.2rem;line-height: 1.2rem;text-align: center;font-size:12px;border: 1px solid #1a8cfe;color: #1a8cfe;border-radius:1.2rem;margin-bottom: 0.4rem;}
.pingjia_info .info_lt .lt:nth-child(2){margin:0 2%;}
.pingjia_info .info_lt .lt:last-child{margin-left: 2%;}
.pingjia_info .title_star i{display: inline-block;width: 0.8rem;height: 0.8rem;margin:0 3px;
background:url(data:image/png;base64,) no-repeat center;background-size: 100%;
}
.pingjia_info .star1 i:first-child,.pingjia_info .star2 i:nth-child(1),.pingjia_info .star2 i:nth-child(2),.pingjia_info .star3 i:nth-child(1),.pingjia_info .star3 i:nth-child(2),.pingjia_info .star3 i:nth-child(3),.pingjia_info .star4 i:nth-child(1),.pingjia_info .star4 i:nth-child(2),.pingjia_info .star4 i:nth-child(3),.pingjia_info .star4 i:nth-child(4),.pingjia_info .star5 i:nth-child(1),.pingjia_info .star5 i:nth-child(2),.pingjia_info .star5 i:nth-child(3),.pingjia_info .star5 i:nth-child(4),.pingjia_info .star5 i:nth-child(5){
background:url(data:image/png;base64,) no-repeat center;background-size: 100%;
}

/* 概况 */
.gaik_info{color: #595959;padding-top: 0.5rem;}

/* 无信息 */
.none_content{text-align: center;}
.none_content image{width:6.0rem;margin:2.0rem 0 0.5rem;}
.none_text{color:#BEBEBE;font-size:0.75rem;}
.none_btn{display: block;width:6.0rem;height:1.5rem;text-align: center;line-height:1.5rem;color:#1A8CFE;border:1px solid #1A8CFE;border-radius:1.0rem;font-size: 0.8rem;margin:0.6rem auto 0;}

